page{
  background-color:#fff;
}
.re-detail-title{
  width:100%;
  height:45px;
  line-height: 45px;
  background-color: #25aab1;
  color:#fff;
  padding-left:25px;
  font-size:17px;
}
.re-detail-card{
  margin:15px;
  padding:10px;
  border-radius: 5px;
  box-shadow:0 3px 18px #d0d0d0;
}
.expert-label{
  font-size:18px;
  color:#466466;
  margin-bottom: 10px;
}
.expert-con{
  font-size:16px;
  color:#555;
  margin-bottom: 10px;
  line-height:28px;
}
.test-next{
  width:100%;
  height:50px;
  text-align: center;
  margin-top:50px;
}
.test-next-btn{
  width:120px;
  height:38px;
  line-height: 38px;
  background-color:#25aab1;
  color:#fff;
  border-radius: 20px;
  display: inline-block;
  font-size:16px;
  text-align: center;
}